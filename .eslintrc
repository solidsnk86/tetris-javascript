{
  "env": {
    "es2022": true,
    "node": true,
    "browser": true
  },
  "extends": ["eslint:recommended", "eslint-config-prettier", "standard"],
  "rules": {
    "space-before-function-paren": "off",
    "no-unused-vars": "warn",
    "no-multiple-empty-lines": "off",
    "no-tabs": "off",
    "indent": ["warn", "tab"],
    "quotes": ["warn", "single"],
    "eol-last": "off"
  }
}
